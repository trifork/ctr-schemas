<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
		xmlns:ctr="http://lmst.dk/Ctr/100"
		targetNamespace="http://lmst.dk/Ctr/100"
		elementFormDefault="qualified" attributeFormDefault="unqualified">

	<element name="PackageNumber" type="ctr:PackageNumberType">
		<annotation>
			<documentation xml:lang="en-GB">Package number</documentation>
			<documentation xml:lang="da-DK">Varenummer</documentation>
		</annotation>
	</element>
	<simpleType name="PackageNumberType">
		<restriction base="string">
			<maxLength value="100"/>
		</restriction>
	</simpleType>
</schema>
