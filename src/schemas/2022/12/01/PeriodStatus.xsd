<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
		xmlns:ctr="http://lmst.dk/Ctr/100"
		targetNamespace="http://lmst.dk/Ctr/100"
		elementFormDefault="qualified" attributeFormDefault="unqualified">
	
	<include schemaLocation="Amount.xsd"/>
	<include schemaLocation="StartDato.xsd"/>
	<include schemaLocation="SlutDato.xsd"/>
	
	<element name="PeriodStatus" type="ctr:PeriodStatusType">
		<annotation>
			<documentation xml:lang="en-GB">Account status for a CTR period</documentation>
			<documentation xml:lang="da-DK">Kontostatus for en CTR periode</documentation>
		</annotation>
	</element>
	<complexType name="PeriodStatusType">
		<sequence>            		
            <element name="StartDato" type="ctr:StartDatoType"/> 
            <element name="SlutDato" type="ctr:SlutDatoType"/>
            <element name="Balance" type="ctr:AmountType"/>
			<element name="UdligningsBeloeb" type="ctr:AmountType" minOccurs="0"/>        
            <element name="UdbetaltTilskud" type="ctr:AmountType" minOccurs="0"/>
            <element name="BerettigetTilskud" type="ctr:AmountType" minOccurs="0"/>            
		</sequence>
	</complexType>
</schema>
