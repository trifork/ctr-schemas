<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
		xmlns:ctr="http://lmst.dk/ctr/xml.schema/2022/12/01"
		targetNamespace="http://lmst.dk/ctr/xml.schema/2022/12/01"
		elementFormDefault="qualified">
	
	<element name="IsChild" type="ctr:IsChildType">
		<annotation>
			<documentation xml:lang="en-GB">Indicates if the given person is a child</documentation>
			<documentation xml:lang="da-DK">Angiver hvorvidt den givne person er et barn</documentation>
		</annotation>
	</element>
	<simpleType name="IsChildType">
		<restriction base="boolean"/>
	</simpleType>
</schema>
