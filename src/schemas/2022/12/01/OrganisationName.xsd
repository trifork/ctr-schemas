<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
		xmlns:ctr="http://lmst.dk/ctr/xml.schema/2022/12/01"
		targetNamespace="http://lmst.dk/ctr/xml.schema/2022/12/01"
		elementFormDefault="qualified" >

<element name="OrganisationName" type="ctr:OrganisationNameType">
		<annotation>
			<documentation xml:lang="en-GB">Organisation name</documentation>
			<documentation xml:lang="da-DK">Organisationsnavn</documentation>
		</annotation>
	</element>
	<simpleType name="OrganisationNameType">
