<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
		xmlns:ctr="http://lmst.dk/Ctr/xml.schema/2022/12/01"
		targetNamespace="http://lmst.dk/Ctr/xml.schema/2022/12/01"
		elementFormDefault="qualified" attributeFormDefault="unqualified">

	<include schemaLocation="PersonIdentifier.xsd"/>
	<include schemaLocation="CreateTransaction.xsd"/>

	<element name="CreateTransactionRequest" type="ctr:CreateTransactionRequestType">
		<annotation>
			<documentation xml:lang="en-GB">Element for creating one or more CTR transactions</documentation>
			<documentation xml:lang="da-DK">Element for at oprette en eller flere CTR transaktioner</documentation>
		</annotation>
	</element>
	<complexType name="CreateTransactionRequestType">
		<sequence>
            <element name="PersonIdentifier" type="ctr:PersonIdentifierType"/>
			<element name="CreateTransaction" type="ctr:CreateTransactionType" maxOccurs="100"/>         
		</sequence>
	</complexType>
</schema>
