		targetNamespace="http://lmst.dk/Ctr/100"
		elementFormDefault="qualified" attributeFormDefault="unqualified">

	<include schemaLocation="PersonIdentifier.xsd"/>   

	<element name="SkiftPersonIdentifierRequest" type="ctr:SkiftPersonIdentifierRequestType">
		<annotation>
			<documentation xml:lang="en-GB">Element for a request for changing the official danish person identifier</documentation>
			<documentation xml:lang="da-DK">Element for en foresp√∏rgsel efter at skifte person identifikation</documentation>
