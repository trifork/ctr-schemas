<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
        xmlns:ctr="http://lmst.dk/Ctr/100"
		targetNamespace="http://lmst.dk/Ctr/100"
        elementFormDefault="qualified" 
        attributeFormDefault="unqualified">
	<element name="OphoersAarsag" type="ctr:OphoersAarsagType">
		<annotation>
			<documentation xml:lang="en-GB">Reason for ending henstandsordninger</documentation>
			<documentation xml:lang="da-DK">Ophørs-årsag for henstandsordninger</documentation>
		</annotation>
	</element>
	<simpleType name="OphoersAarsagType">
		<union memberTypes="ctr:PredefinedOphoersAarsagType ctr:UndefinedOphoersAarsagType"/>
	</simpleType>
	<simpleType name="PredefinedOphoersAarsagType">
		<restriction base="ctr:UndefinedOphoersAarsagType">
			<enumeration value="ukendt"/>
			<enumeration value="mislighold"/>
        </restriction>
	</simpleType>
	<simpleType name="UndefinedOphoersAarsagType">
		<restriction base="string">
			<minLength value="1"/>
			<maxLength value="10"/>
		</restriction>
	</simpleType>	
</schema>
