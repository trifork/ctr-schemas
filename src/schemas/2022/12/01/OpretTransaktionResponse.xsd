<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
		xmlns:ctr="http://lmst.dk/Ctr/100"
		targetNamespace="http://lmst.dk/Ctr/100"
		elementFormDefault="qualified" attributeFormDefault="unqualified">

	<include schemaLocation="PatientStatus.xsd"/>
    <include schemaLocation="PersonIdentifier.xsd"/>
    
	<element name="OpretTransaktionResponse" type="ctr:OpretTransaktionResponseType">
		<annotation>
			<documentation xml:lang="en-GB">Outer container element for a response after creating a CTR transaction</documentation>
			<documentation xml:lang="da-DK">Ydre container-element for et svar efter at have oprettet en CTR transaktion</documentation>
		</annotation>
	</element>
	<complexType name="OpretTransaktionResponseType">
		<sequence>
            <element name="PersonIdentifier" type="ctr:PersonIdentifierType"/>
            <element name="PersonIdentifierSource" type="ctr:PersonIdentifierSourceType"/>
            <element name="UdligningsBeloeb" type="ctr:Money"/>
            <element name="Saldo" type="ctr:Money"/>
			TODO add warnings ??
		</sequence>
	</complexType>
</schema>
