	<include schemaLocation="PersonIdentifier.xsd"/>
    <include schemaLocation="RequestedPeriod.xsd"/>
	<include schemaLocation="AccountName.xsd"/>

	<element name="GetAccountStatusRequest" type="ctr:GetAccountStatusRequestType">
		<annotation>
			<documentation xml:lang="en-GB">Element for a request for account status</documentation>
			<documentation xml:lang="da-DK">Element for en foresp√∏rgsel efter kontostatus</documentation>
		</annotation>
	</element>
	<complexType name="GetAccountStatusRequestType">
		<sequence>
            <element name="PersonIdentifier" type="ctr:PersonIdentifierType"/>            
            <element name="AccountName" type="ctr:AccountNameType" maxOccurs="100"/>
            <element name="Period" type="ctr:RequestedPeriodType"/>
		</sequence>
	</complexType>
</schema>
